{
	"name": "EditAccount",
	"version": "3.0.0-alpha",
	"author": [
		"Łukasz Garczewski",
		"Jack Phoenix",
		"WikiTeq"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:EditAccount",
	"descriptionmsg": "editaccount-desc",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.36.0"
	},
	"license-name": "GPL-2.0-or-later",
	"GroupPermissions": {
		"*": {
			"editaccount": false
		},
		"staff": {
			"editaccount": true
		}
	},
	"AvailableRights": [
		"editaccount"
	],
	"SpecialPages": {
		"EditAccount": {
			"class": "EditAccount",
			"services": [
				"PasswordFactory",
				"UserNameUtils",
				"UserOptionsManager",
				"MainWANObjectCache"
			]
		},
		"CloseAccount": {
			"class": "CloseAccount",
			"services": [
				"UserGroupManager",
				"UserNameUtils"
			]
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\EditAccount\\": "includes/"
	},
	"LogTypes": [
		"editaccnt"
	],
	"LogRestrictions": {
		"editaccnt": "editaccount"
	},
	"LogActionsHandlers": {
		"editaccnt/*": "LogFormatter"
	},
	"MessagesDirs": {
		"EditAccount": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"EditAccountAliases": "EditAccount.alias.php"
	},
	"AutoloadClasses": {
		"CloseAccount": "includes/Specials/SpecialCloseAccount.php",
		"EditAccount": "includes/Specials/SpecialEditAccount.php",
		"EditAccountHooks": "includes/Hooks.php",
		"User": "includes/User.php"
	},
	"Hooks": {
		"AdminLinks": "MediaWiki\\Extension\\EditAccount\\EditAccountHooks::addToAdminLinks"
	},
	"ResourceModules": {
		"ext.editAccount": {
			"localBasePath": "modules",
			"remoteExtPath": "EditAccount/modules",
			"styles": [
				"editAccount.css"
			]
		},
		"ext.editAccount.displayuser": {
			"localBasePath": "modules/js",
			"remoteExtPath": "EditAccount/modules/js",
			"packageFiles": [
				"displayuser.js"
			],
			"dependencies": [
				"jquery"
			]
		}
	},
	"manifest_version": 1
}
